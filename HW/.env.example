SECRET_KEY=change-this-to-a-random-32byte-string

# Primary admin credentials
ADMIN_USERNAME=admin
# Hash created with Werkzeug generate_password_hash (method pbkdf2:sha256)
ADMIN_PASSWORD_HASH=pbkdf2:sha256:260000$example$6b3a2bb59f5c3e2c96ef9f...trimmed

# Optional: default role for first login users
DEFAULT_ROLE=user

# Self-registration
REGISTER_OPEN=true
REGISTER_INVITE_CODE=

# Password reset
RESET_TOKEN_TTL_MIN=20
RESET_DEV_SHOW_TOKEN=true